# This ssh config file defines the settings for common server connection.
# OSU CSE stdlinux server
Host stdlinux
    HostName stdlinux.cse.ohio-state.edu
    Port 22
    User yang.736
    IdentityFile ~/.ssh/stdlinux.key
# OSC oakley account
Host osc
    HostName oakley.osc.edu
    Port 22
    User osu8274
# Chameleon cloud GPU server
Host chameleonGPU
    HostName 129.114.109.171
    Port 22
    User cc
    IdentityFile ~/.ssh/gpucloudkey.pem
    ForwardX11 yes
    ForwardAgent yes
# Amazon EC2 server
Host ec2
    HostName ec2-54-245-190-212.us-west-2.compute.amazonaws.com
    Port 22
    User ubuntu
    IdentityFile ~/.ssh/zhy028-cse260-wi18.pem
